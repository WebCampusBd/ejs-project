{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "view": {
      "defaultEngine": "ejs",
      "ext": ".ejs",
      "name": "home",
      "root": "/var/task/views"
      }
  }